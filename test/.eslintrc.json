{
  "env": {
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "require-jsdoc": "off",
    "max-len": "off",
    "quote-props": "off",
    "no-new": "off",
    "valid-jsdoc": "off",
    "no-unused-expressions": "off",
    "class-methods-use-this": "off"
  }
}
