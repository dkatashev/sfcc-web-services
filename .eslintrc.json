{
  "root": true,
  "extends": "airbnb-base/legacy",
  "globals": {
    "XML": "readonly",
    "webreferences2": "readonly"
  },
  "rules": {
    "max-len": "off",
    "vars-on-top": "off",
    "no-underscore-dangle": "off",
    "func-names": "off",
    "global-require": "off",
    "no-param-reassign": "off",
    "comma-dangle": "off"
  }
}
