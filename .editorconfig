# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
quote_type = single

[{package.json,package-lock.json.travis.yml}]
indent_size = 2
indent_style = space

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false
